# Cloud Server Environment Variables
# Copy to .env and configure

HOST=0.0.0.0
PORT=8000
DEBUG=false

# Database
DATABASE_URL=sqlite+aiosqlite:///./autonova.db

# Redis (optional)
REDIS_URL=redis://localhost:6379

# JWT Security (CHANGE IN PRODUCTION!)
JWT_SECRET=autonova-jwt-secret-change-in-production-12345
JWT_ALGORITHM=HS256

# Encryption Key (MUST MATCH CLIENT!)
ENCRYPTION_KEY=autonova-rmm-secret-key-change-me-12345
